{
  "version": 2,
  "name": "checkout-syncpay",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "SYNCPAY_CLIENT_ID": "",
    "SYNCPAY_CLIENT_SECRET": ""
  }
}
